function reverse(HashMap graph):\
	create HashMap M\
	for key in graph:\
		vertex v = key\
		put new set at v in new map M\
		for values at key in graph:\
			put value in set at v in new map M\
\
b. \
function nextAdjacentVertices(Map graph, Vertex source):\
	create Set S\
	for adjacency a of source:\
		for adjacency b of a:\
			add b to S\
	return S\
